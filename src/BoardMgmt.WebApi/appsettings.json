{
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Jwt": {
    "Issuer": "BoardMgmt",
    "Audience": "BoardMgmt.Client",
    "Key": "",
    "ExpiryHours": "24"
  },
  "Uploads": {
    "Provider": "Local",
    "PublicBase": "/uploads",
    "PhysicalRoot": ""
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:4200",
      "http://localhost:5174"
    ]
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.MSSqlServer"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning",
        "Microsoft.EntityFrameworkCore.Database.Command": "Warning",
        "Microsoft.EntityFrameworkCore.Update": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionStringName": "DefaultConnection",
          "tableName": "Logs",
          "schemaName": "dbo",
          "autoCreateSqlTable": true,
          "batchPostingLimit": 50,
          "period": "00:00:02"
        }
      },
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ]
  },
  "Graph": {
    "TenantId": "",
    "ClientId": "",
    "ClientSecret": "",
    "MailboxAddress": "",
    "WebhookNotificationUrl": "",
    "WebhookClientState": ""
  },
  "Zoom": {
    "AccountId": "",
    "ClientId": "",
    "ClientSecret": "",
    "HostUserId": "",
    "WebhookSecretToken": ""
  },
  "Seed": {
    "AdminEmail": "",
    "AdminPassword": ""
  },
  "App": {
    "AppBaseUrl": "http://localhost:4200",
    "MailboxAddress": "",
    "MailboxAddressZoom": ""
  },
  "Smtp": {
    "Host": "",
    "Port": 587,
    "EnableSsl": true,
    "Username": "",
    "Password": "",
    "DefaultFrom": "",
    "FromDisplayName": "BoardMgmt"
  },
  "AllowedHosts": "*"
}
